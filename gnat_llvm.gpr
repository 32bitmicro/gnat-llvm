project Gnat_LLVM is
   for Source_Dirs use ("gnat_src", "llvm-ada/out", "llvm-backend", "llvm-backend/obj");
   for Object_Dir use "llvm-backend/obj";
   for Languages use ("Ada");

   package Compiler is
      for Default_Switches ("Ada") use
        ("-g", "-O0", "-gnata", "-gnatVa", "-gnatQ", "-gnatygO",
         "-gnatwaCJe", "-gnat2012", "-gnateE");
   end Compiler;

end Gnat_LLVM;
